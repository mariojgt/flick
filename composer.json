{
    "name": "mariojgt/flick",
    "description": "A laravel page buidler out of the box",
    "keywords": [
        "laravel",
        "grapejs",
        "grape js",
        "ck editor",
        "laravel page builder",
        "page builder",
        "builder"
    ],
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "mariojgt",
            "email": "mariojgt2@gmail.com"
        }
    ],
    "require":{
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mariojgt\\Flick\\": "src"
        }
    },

    "scripts": {
        "post-autoload-dump": [
            "@php artisan route:list"
        ]
    },

    "extra": {
        "laravel": {
            "providers": [
                "Mariojgt\\Flick\\FlickProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
